{
  "version": 2,
  "builds": [
    {
      "src": "sentiabot-frontend/package.json",
      "use": "@vercel/static-build"
    },
    {
      "src": "api/chat.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/chat",
      "dest": "/api/chat.ts"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/sentiabot-frontend"
    }
  ]
}