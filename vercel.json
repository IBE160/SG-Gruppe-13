{
  "version": 2,
  "builds": [
    {
      "src": "sentiabot-frontend/package.json",
      "use": "@vercel/static-build"
    },
    {
      "src": "api/chat.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}